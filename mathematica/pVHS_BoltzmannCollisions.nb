Cell[TextData[{
 "The following is because:\n- for a trace-free \[Sigma] we have\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"i", ",", "j"}]], 
     RowBox[{
      SubscriptBox["\[Sigma]", "ij"], 
      SubscriptBox["\[Sigma]", "ij"]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"2", 
       SubscriptBox["\[Sigma]", "xx"], 
       SubscriptBox["\[Sigma]", "xx"]}], "+"}], "..."}]}], TraditionalForm]],
  ExpressionUUID->"1b6f8a91-cc63-40bf-bfa1-7c80737c8444"],
 "\n- and \n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Sum]", "k"], 
     RowBox[{
      SubscriptBox["\[Sigma]", 
       RowBox[{"k", "<", "i"}]], 
      SubscriptBox["\[Sigma]", 
       RowBox[{"j", ">", "k"}]]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["1", "3"], 
       SubscriptBox["\[Sigma]", "xx"], 
       SubscriptBox["\[Sigma]", "xx"]}], "+"}], "..."}]}], TraditionalForm]],
  ExpressionUUID->"0a9d8f36-2162-4d38-98a2-cd8948cec803"],
 "\n- and for any two vector u and v we have\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["u", 
      RowBox[{"<", "i"}]], 
     SubscriptBox["v", 
      RowBox[{"j", ">"}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["2", "3"], 
       SubscriptBox["u", "x"], 
       SubscriptBox["v", "x"]}], "+"}], "..."}]}], TraditionalForm]],
  ExpressionUUID->"b13a56ee-860f-45ff-b488-6e3997865167"]
}], "Text",ExpressionUUID->"d15f0f68-30e7-47ed-8382-ad25030834c1"],

